{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            //pytest-debugging-config
            "console": "integratedTerminal",
            "cwd": "${fileWorkspaceFolder}",
            "env": {
                "PYTEST_ADDOPTS": "--no-cov"
            },
            "justMyCode": false,
            "name": "Python: Debug",
            "purpose": [
                "debug-test"
            ],
            "request": "launch",
            "type": "python"
        },
        {
            "name": "Hydra",
            "type": "python",
            "request": "launch",
            "module": "lstm_autoencoder.main",
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "1",
                "HYDRA_FULL_ERROR": "1"
            },
            "autoReload": {
                "enable": true
            },
            "args": [],
            // Any other arguments should be passed as a comma-seperated-list
            "justMyCode": true
        },
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "1"
            },
            "autoReload": {
                "enable": true
            },
            "justMyCode": true
        }
    ]
}
